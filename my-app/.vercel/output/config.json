{
	"version": 3,
	"routes": [
		{
			"src": "/_app/immutable/.+",
			"headers": {
				"cache-control": "public, immutable, max-age=31536000"
			}
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^/install/?(?:/__data.json)?$",
			"dest": "/install"
		},
		{
			"src": "^/mods/?(?:/__data.json)?$",
			"dest": "/mods"
		},
		{
			"src": "/.*",
			"dest": "/![-]/catchall"
		}
	],
	"overrides": {
		"index.html": {
			"path": ""
		}
	}
}